<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smec.tam">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:background="@color/background"
        android:icon="@mipmap/tam_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tam_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InterClgReg"></activity>
        <activity android:name=".RecruitRegActivity"></activity>
        <activity android:name=".ResultsActivity" />
        <activity android:name=".NoInternet" />
        <activity android:name=".EventDetailsActivity" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/NoBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registration" />
        <activity android:name=".HomePage" />
    </application>

</manifest>